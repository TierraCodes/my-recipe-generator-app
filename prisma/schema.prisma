// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}
model Recipe {
  id          Int      @id @default(autoincrement())
  name        String
  description String
}
model CookingMethod {
  id      Int      @id @default(autoincrement())
  name    String
  length  String
}
model Ingredient {
  id      Int      @id @default(autoincrement())
  name    String
  amount  String
}
model PreparationMethod {
  id      Int      @id @default(autoincrement())
  name    String
  length  String
}
model Equipment {
  id      Int      @id @default(autoincrement())
  name    String
}